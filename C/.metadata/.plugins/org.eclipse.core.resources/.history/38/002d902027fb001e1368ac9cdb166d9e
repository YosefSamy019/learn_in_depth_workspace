 */


#include <stdio.h>

void main(){
	unsigned int no = 0;

	printf("Enter Example No: ");
	scanf("%d",&no);

	switch(no){
	case 1:
		{
			printf("C Programming");
			break;
		}
	case 2:
		{
			int number;

			printf("Enter an Integer: ");
			scanf("%d",&number);

			printf("You Entered %d",number);
			break;
		}
	case 3:
		{
			int num1,num2;

			printf("Enter two Integer: ");
			scanf("%d%d",&num1,&num2);

			printf("Sum: %d",num1 + num2);
			break;
		}
	case 4:
		{
			float num1,num2;

			printf("Enter two float: ");
			scanf("%f%f",&num1,&num2);

			printf("Product: %f",num1 * num2);
			break;
		}
	}
}
