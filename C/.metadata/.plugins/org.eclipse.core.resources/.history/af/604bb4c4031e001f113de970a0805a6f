#include <stdio.h>

struct SStudent{
	char name[20];
	int rollNo;
	float marks;
};

void main(){
	struct SStudent s;

	printf("Enter inforamtion of students: \n");

	printf("Enter Name: ");
	gets(s.name);

	printf("Enter roll number: ");
	scanf("%d",&s.rollNo);

	printf("Enter Name: ");
	scanf("%f",&s.marks);

	printf("\Display inforamtion \n");

	printf("Name: %s\n",s.name);
	printf("Roll: %d\n",s.rollNo);
	printf("Marks: %f\n",s.marks);

}

//-------- EX1-------
