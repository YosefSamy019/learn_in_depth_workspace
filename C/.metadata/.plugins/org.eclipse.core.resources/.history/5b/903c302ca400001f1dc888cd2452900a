#include <stdio.h>

unsigned long long fact(int num) {
	if (num <= 1)
		return 1;
	else
		return num * fact(num - 1);
}

int main(void) {
	int min, max, i;
	char found = 0;

	printf("Enter 2 numbers (intervals): ");
	fflush(stdin);
	fflush(stdout);
	scanf("%d %d", &min, &max);

	for (i = min; i <= max; i++) {
		if (isPrime(i)) {
			found = 1;
			printf("%d ", i);
		}
	}

	if (found == 0) {
		printf("No number found\n");
	}

	return 0;
}
