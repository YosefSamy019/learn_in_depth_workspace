#include<stdio.h>

void ex_1(){
	int m = 29;
	printf("Addess of m: 0x%x\nValue of m: %d",&m,m);


	int* ab = &m;
	printf("\n\nAddess of pointer: 0x%x\nValue of pointer: %d",ab,*ab);


	m = 34;
	printf("\n\nThe value of m assigned to 34 now\n");

	printf("Addess of pointer: 0x%x\nValue of pointer: %d",ab,*ab);


}




